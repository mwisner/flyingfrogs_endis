<config> 
  <modules>
    <FlyingFrogs_Endis>
      <version>0.1.0</version>
    </FlyingFrogs_Endis>
  </modules>
  <crontab>
    <jobs>
      <FlyingFrogs_Endis>
        <schedule>
          <cron_expr>*/5 * * * *</cron_expr>
        </schedule>
        <run>
          <model>endis/cron::dailyProductEnDis</model>
        </run>
      </FlyingFrogs_Endis>
    </jobs>
  </crontab>
  <frontend>
    <routers>
      <endis>
	<use>standard</use>
	<args>
	  <module>FlyingFrogs_Endis</module>
	  <frontName>endis</frontName>
	</args>
      </endis>
    </routers>
  </frontend>
  <global>
    <models>
      <endis>
	<class>FlyingFrogs_Endis_Model</class>
	<resourceModel>tis_mysql4</resourceModel>
      </endis>
      <tis_mysql4>
	<class>FlyingFrogs_Endis_Model_Mysql4</class>
	<entities>
	  <endis>
	    <table>tis</table>
	  </endis>
	</entities>
      </tis_mysql4>
    </models>
    <resources>
      <endis_setup>
	<setup>
	  <module>FlyingFrogs_Endis</module>
	</setup>
	<connection>
	  <use>core_setup</use>
	</connection>
      </endis_setup>
      <endis_write>
	<connection>
	  <use>core_write</use>
	</connection>
      </endis_write>
      <endis_read>
	<connection>
	  <use>core_read</use>
	</connection>
      </endis_read>
    </resources>
    <blocks>
      <endis>
	<class>FlyingFrogs_Endis_Block</class>
      </endis>
    </blocks>
    <helpers>
      <endis>
	<class>FlyingFrogs_Endis_Helper</class>
      </endis>
    </helpers>
  </global>
</config>
